        <th scope="col">Code</th>
        <th scope="col">Title</th>
        <th scope="col">Category</th>
        <th scope="col" style="text-align: center">Options</th>
      </tr>
    </thead>
    <tbody>
    <% (1..4).each do |i| %>
      <tr>
        <td>TOA001</td>
        <td>De toan 001</td>
