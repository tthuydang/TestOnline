<div>
  <%= render "shared/navigation" %>
  <%= image_tag("cambridge_banner.jpg") %>
  <p class="notice"><% notice %></p>
  <p class="alert"><% alert %> </p>
</div>
<div class="container">
  <div class="row" style="padding-top:0%">
    <div class="col-md-2">
      <%= render "shared/menu" %>
    </div>
    <div class="col-md-8">
      <%= render("shared/alert") %>

<h2 style="color: gray">Your Tickets:</h2>
<div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Code</th>
        <th scope="col">Title</th>
        <th scope="col">Category</th>
        <th scope="col" style="text-align: center">Options</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: "/creator/tickets/shared/tickets", collection: @tickets, as: :ticket %>
    </tbody>
  </table>

  <%= link_to("Add new Ticket", new_ticket_path, class: "btn btn-success") %>
</div>

    </div>
    <div class="col-md-2"></div>
  </div>
</div>
<div class="container">
  <div class="row" style="padding-top:5%;">
    <%= render("shared/footer") %>
  </div>
</div>
